}

hr {
  border: 0;
  border-top-width: 1px;
  border-color: #e2e8f0;
}

/* Accessibility */
.visually-hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  border: 0;
}

.accessible {
  position: relative;
  overflow: visible;
  clip: auto;
}

