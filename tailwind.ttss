/* Forms */
input,
textarea,
select {
  appearance: none;
  border-radius: 4px;
  border-width: 1px;
  border-color: #e2e8f0;
  padding: 8px 12px;
  font-size: 16px;
  font-family: system-ui, sans-serif;
}

input:focus,
textarea:focus,
select:focus {
  outline: 2px solid #3182ce;
  outline-offset: 2px;
}

input:disabled,
textarea:disabled,
select:disabled {
  background-color: #edf2f7;
  cursor: not-allowed;
}

textarea {
  resize: vertical;
}

select {
  padding-right: 40px;
}

fieldset {
  border-width: 0;
  padding: 0;
  margin: 0;
}

label {
  display: block;
  margin-bottom: 8px;
  font-weight: 600;
}

button {
  background-color: #3182ce;
  color: #fff;
  border: 0;
  border-radius: 4px;
  padding: 8px 16px;
  font-size: 16px;
  font-family: system-ui, sans-serif;
  cursor: pointer;
}

button:disabled {
  background-color: #e2e8f0;
  cursor: not-allowed;
}

/* Tables */
table {
  width: 100%;
  border-collapse: collapse;
}

